t=int(input())
for _ in range(t):
    a,b,c=map(int,input().split())
    if(a>b):
        if(b>c):
            return b
        else:
            return c
    else:
        if(b<c):
            return b
        else:
            if(c>a):
                return c
            else:
                return a
